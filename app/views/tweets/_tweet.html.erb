<div id="<%= dom_id tweet %>">
  <p>
    <strong>Body:</strong>
    <%= tweet.body %>
  </p>

  <p>
    <strong>Replies count:</strong>
    <%= tweet.replies_count %>
  </p>

  <p>
    <strong>Likes count:</strong>
    <%= tweet.likes_count %>
  </p>

  <p>
    <strong>Name</strong>
    <%= tweet.user.name %>
  </p>
  <p>
    <strong>Username</strong>
    <%= tweet.user.username %>
  </p>


</div>
